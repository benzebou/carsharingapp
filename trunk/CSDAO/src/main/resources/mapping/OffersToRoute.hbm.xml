<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="cs.model">
    <class name="OffersToRoute" table="offers_to_routes">
        <id name="id" column="_id_offers_to_route">
            <generator class="native"/>
        </id>
        <property name="idOffer" type="integer" column="_id_offer"/>
        <property name="idRoute" type="integer" column="_id_route"/>
     </class>
</hibernate-mapping>