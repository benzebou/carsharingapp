<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="cs.model">
    <class name="RouteToCustomerAccount" table="routes_to_customer_accounts">
        <id name="id" column="_id_routes_to_customer_acount">
            <generator class="native"/>
        </id>
        <property name="idRoute" type="integer" column="_id_route"/>
        <property name="idCustomerAccount" type="integer" column="_id_customer_account"/>
     </class>
</hibernate-mapping>